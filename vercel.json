{
    "version":2,
    "builds":[
        {
            "src": "clg_projects/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.8.6rc1"
            }
        },
        {
            "src": "build.sh",
            "use": "@vercel/static-build",
            "config":{
                "disDir": "static_build"
            }

        }
    ],

    "rewrites":[
        {"source": "/(.*)", "destination": "/"}
    ]
}