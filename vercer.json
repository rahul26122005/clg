{
    "version":2,
    "builds":[
        {
            "src": "clg_projects/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.7"
            }
        },
        {
            "src": "build.sh",
            "use": "@vercel/static-build",
            "config":{
                "disDir": "static_build"
            }

        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"  
        },
        {
            "src": "/(.*)",
            "dest": "clg_projects/wsgi.py"
        }
    ]
}